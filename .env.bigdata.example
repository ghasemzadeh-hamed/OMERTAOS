# Kafka
KAFKA_BROKERS=kafka:9092
KAFKA_SCHEMA_REGISTRY_USER=schema_registry
KAFKA_SCHEMA_REGISTRY_PASSWORD=changeit
KAFKA_SASL_USERNAME=router
KAFKA_SASL_PASSWORD=router-secret
KAFKA_LOG_RETENTION_HOURS=72

# Schema Registry
SCHEMA_REGISTRY_URL=http://schema-registry:8081

# ClickHouse
CLICKHOUSE_URL=http://clickhouse:8123
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=

# MinIO
MINIO_ENDPOINT=http://minio:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_RAW_EXPIRE_DAYS=3
MINIO_PROCESSED_EXPIRE_DAYS=14

# Airflow
AIRFLOW_HOME=/opt/airflow
AIRFLOW__CORE__LOAD_EXAMPLES=false
AIRFLOW_ADMIN_PASSWORD=airflow

# Spark
SPARK_MODE=local[*]
SPARK_MASTER_URL=spark://spark-master:7077

# Superset
SUPERSET_SECRET_KEY=please-change-me
SUPERSET_LOAD_TEST_DATA=false

# Grafana
GRAFANA_ADMIN_PASSWORD=admin

# Redis
REDIS_HOST=redis

# Control Plane Hooks
CONTROL_PLANE_BASE_URL=http://control:8000
