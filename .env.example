# aionOS core environment example
NODE_ENV=development

# Gateway
AION_GATEWAY_PORT=8080
AION_GATEWAY_HOST=0.0.0.0
AION_CONTROL_GRPC=control:50051
AION_REDIS_URL=redis://redis:6379/0
AION_GATEWAY_API_KEYS=demo-key:admin|manager
AION_RATE_LIMIT_MAX=60
AION_RATE_LIMIT_PER_IP=120
AION_RATE_LIMIT_PER_USER=90
AION_RATE_LIMIT_WINDOW=1 minute
AION_IDEMPOTENCY_TTL=900
AION_TLS_ENABLED=false
AION_TLS_CERT_PATH=
AION_TLS_KEY_PATH=
AION_TLS_CA_CHAIN=
AION_MTLS_ENABLED=false
AION_MTLS_CA_PATH=
AION_TLS_REQUIRE_MTLS=false
AION_OUTBOUND_WEBHOOKS=http://localhost:9500/webhook|super-secret|task.status.changed,chat.session.created|3|5
AION_INBOUND_WEBHOOK_SECRET=super-secret
AION_JWT_PUBLIC_KEY=
AION_JWT_ISSUER=aionOS
AION_JWT_AUDIENCE=aion-clients
TENANCY_MODE=single
AION_REQUIRE_X_TENANT=false
AION_CONSOLE_USERS=admin@aionos.dev:changeme:admin|manager
AION_CONSOLE_GATEWAY_API_KEY=demo-key
AION_CONSOLE_DEFAULT_TENANT=default

# Control plane
AION_CONTROL_POSTGRES_DSN=postgresql://postgres:postgres@postgres:5432/aion
AION_CONTROL_REDIS_URL=redis://redis:6379/1
AION_CONTROL_MONGO_DSN=mongodb://mongo:27017/aion
AION_CONTROL_QDRANT_URL=http://qdrant:6333
AION_CONTROL_MINIO_ENDPOINT=http://minio:9000
AION_CONTROL_MINIO_ACCESS_KEY=admin
AION_CONTROL_MINIO_SECRET_KEY=admin123
AION_CONTROL_KAFKA_BROKERS=kafka:9092
AION_CONTROL_SCHEMA_REGISTRY_URL=http://schema-registry:8081
MONGO_TTL_DAYS=14
CLICKHOUSE_TTL_DAYS=90
MINIO_LIFECYCLE_JSON=

# Console
CONSOLE_PORT=3000
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=change-me
NEXT_PUBLIC_GATEWAY_URL=http://localhost:8080
NEXT_PUBLIC_CONTROL_URL=http://localhost:8081
NEXT_PUBLIC_GRAFANA_URL=http://localhost:3001
NEXT_PUBLIC_CONSOLE_GATEWAY_KEY=demo-key
NEXT_PUBLIC_STREAM_PROTOCOL=sse
CONSOLE_GATEWAY_API_KEY=demo-key
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Observability
OTEL_EXPORTER_OTLP_ENDPOINT=http://otel-collector:4317
OTEL_SERVICE_NAME=gateway
PROMETHEUS_SCRAPE_INTERVAL=15s

# Big data integration
KAFKA_BROKERS=kafka:9092
SCHEMA_REGISTRY_URL=http://schema-registry:8081
CLICKHOUSE_URL=http://clickhouse:8123
MINIO_ENDPOINT=http://minio:9000
MINIO_ACCESS_KEY=admin
MINIO_SECRET_KEY=admin123
SPARK_MASTER_URL=spark://spark-master:7077
